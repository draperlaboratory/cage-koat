eval(x, y) -> eval(x - 1, y) [ x > 0 /\ x > 0 ]
eval(x, y) -> eval(x - 1, y) [ y > 0 /\ x > 0 ]
eval(x, y) -> eval(x, y - 1) [ x > 0 /\ x <= 0 /\ y > 0 ]
eval(x, y) -> eval(x, y - 1) [ y > 0 /\ x <= 0 /\ y > 0 ]
eval(x, y) -> eval(x, y) [ x > 0 /\ x <= 0 /\ y <= 0 ]
eval(x, y) -> eval(x, y) [ y > 0 /\ x <= 0 /\ y <= 0 ]
