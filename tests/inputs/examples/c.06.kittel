eval(an, bn, cn, i, j) -> eval(an, bn, cn + 1, i, j + 1) [ bn > j /\ an > i ]
eval(an, bn, cn, i, j) -> eval(an, bn, cn + 1, i + 1, j) [ bn > j /\ an > i ]
eval(an, bn, cn, i, j) -> eval(an, bn, cn + 1, i + 1, j) [ j >= bn /\ an > i ]
eval(an, bn, cn, i, j) -> eval(an, bn, cn + 1, i, j + 1) [ bn > j /\ i >= an ]
